{
  "name": "canonjs",
  "version": "0.1.0",
  "description": "Javascript wrapper around Canon's CCAPI (Camera Control API)",
  "main": "dist/canonjs.cjs.js",
  "module": "dist/canonjs.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/canonjs.esm.js",
      "require": "./dist/canonjs.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "type-check": "tsc --noEmit"
  },
  "author": "Jon-Eric Cook",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.3"
  },
  "keywords": [
    "canon",
    "ccapi",
    "javascript"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.20.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
