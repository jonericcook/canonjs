{
  "name": "canonjs",
  "version": "0.1.0",
  "description": "Javascript wrapper around Canon's CCAPI (Camera Control API)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "type": "module",
  "scripts": {
    "prepare": "tsup src/index.ts --format cjs,esm --dts",
    "build": "tsup src/index.ts --format cjs,esm --dts"
  },
  "author": "Jon-Eric Cook",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.3"
  },
  "devDependencies": {
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
